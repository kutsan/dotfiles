# Turn off the output unless something tells the opposite.
quiet = on

# Force to display the progress bar in any verbosity.
show_progress = on

# Add a '.html' extension to 'text/html' or a '.css' extension to 'text/css' files that lack one.
adjust_extension = on

# Ignore 'robots.txt' and '<meta name=robots content=nofollow>'.
robots = off

# Print the HTTP and FTP response headers.
# server_response = on
