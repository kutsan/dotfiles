defaults
  tls on
  auth on

account com.kutsankaplan
  host smtp.zoho.com
  port 587
  from me@kutsankaplan.com
  user me@kutsankaplan.com
  passwordeval gpg --no-tty --for-your-eyes-only --quiet --decrypt ~/.config/neomutt/accounts/com.kutsankaplan/secret.gpg
  logfile ~/.config/neomutt/cache/com.kutsankaplan/msmtp.log

account default : com.kutsankaplan
