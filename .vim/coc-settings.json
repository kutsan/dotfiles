{
	"coc.preferences.autoTrigger": "always",
	"coc.preferences.triggerAfterInsertEnter": false,
	"coc.preferences.timeout": 2000,
	"coc.preferences.noselect": false,
	"coc.preferences.formatOnType": false,
	"coc.preferences.diagnostic.enable": true,
	"coc.preferences.diagnostic.signOffset": 1000,
	"coc.preferences.diagnostic.errorSign": "•",
	"coc.preferences.diagnostic.warningSign": "•",
	"coc.preferences.diagnostic.infoSign": "•",
	"coc.preferences.diagnostic.hintSign": "•",
	"coc.preferences.preferCompleteThanJumpPlaceholder": false,
	"coc.preferences.messageLevel": "more",
	"coc.preferences.numberSelect": false,
	"coc.preferences.fixInsertedWord": true,
	"coc.preferences.openResourceCommand": "split",
	"coc.preferences.jumpCommand": "split",
	"coc.preferences.codeLens.enable": false,
	"snippets.ultisnips.directories": ["snippet"]
}
